<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="scenario"
    nsURI="http://com.mmxlabs.lng.emf" nsPrefix="com.mmxlabs.lng.emf">
  <eClassifiers xsi:type="ecore:EClass" name="Scenario">
    <eStructuralFeatures xsi:type="ecore:EReference" name="fleetModel" eType="#//fleet/FleetModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="portModel" eType="#//port/PortModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="cargoModel" eType="#//cargo/CargoModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="contractModel" eType="#//contract/ContractModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="scheduleModel" eType="#//schedule/ScheduleModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="marketModel" eType="#//market/MarketModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="distanceModel" eType="#//port/DistanceModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="canalModel" eType="#//port/CanalModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="optimisation" eType="#//optimiser/Optimisation"
        containment="true"/>
  </eClassifiers>
  <eSubpackages name="fleet" nsURI="http://com.mmxlabs.lng.emf/fleet" nsPrefix="com.mmxlabs.lng.emf.fleet">
    <eClassifiers xsi:type="ecore:EClass" name="FleetModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="fleet" upperBound="-1"
          eType="#//fleet/Vessel" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="vesselClasses" upperBound="-1"
          eType="#//fleet/VesselClass" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Vessel">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="class" eType="#//fleet/VesselClass"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="startRequirement" lowerBound="1"
          eType="#//fleet/PortAndTime" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="endRequirement" lowerBound="1"
          eType="#//fleet/PortAndTime" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="VesselClass">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="capacity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minSpeed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxSpeed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="baseFuelUnitPrice" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ladenAttributes" eType="#//fleet/VesselStateAttributes"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ballastAttributes" eType="#//fleet/VesselStateAttributes"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minHeelVolume" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The volume of cargo below which boil-off cannot be used for fuel. Some vessels cannot access LNG below a certain level in their tanks for fuel."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="fillCapacity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"
          defaultValueLiteral="0.958">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The ship fill capacity, as a proportion (from 0 to 1). Ship fill capacity is the percentage of vessel cargo capacity that can actually be used. Typically this is 98.5%."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="FuelConsumptionLine">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Represents a point on the fuel consumption curve, so a containing VesselStateAttributes will consume the associated amount of fuel at the given speed."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="speed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="consumption" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="VesselStateAttributes">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="vesselState" eType="#//fleet/VesselState"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="nboRate" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="idleNBORate" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="idleConsumptionRate"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="fuelConsumptionCurve"
          upperBound="-1" eType="#//fleet/FuelConsumptionLine" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="VesselState">
      <eLiterals name="Laden"/>
      <eLiterals name="Ballast"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PortAndTime">
      <eStructuralFeatures xsi:type="ecore:EReference" name="port" eType="#//port/Port"
          unsettable="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"
          unsettable="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="schedule" nsURI="http://com.mmxlabs.lng.emf/schedule" nsPrefix="com.mmxlabs.lng.emf.schedule">
    <eClassifiers xsi:type="ecore:EClass" name="ScheduleModel"/>
  </eSubpackages>
  <eSubpackages name="port" nsURI="http://com.mmxlabs.lng.emf/port" nsPrefix="com.mmxlabs.lng.emf.port">
    <eClassifiers xsi:type="ecore:EClass" name="PortModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="ports" upperBound="-1"
          eType="#//port/Port" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Port">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="market" eType="#//market/Market"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DistanceModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="distances" upperBound="-1"
          eType="#//port/DistanceLine" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DistanceLine">
      <eStructuralFeatures xsi:type="ecore:EReference" name="fromPort" eType="#//port/Port"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="toPort" eType="#//port/Port"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="distance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Canal">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="distance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="entryDistances" upperBound="-1"
          eType="#//port/PartialDistance" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="exitDistances" upperBound="-1"
          eType="#//port/PartialDistance" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CanalModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="canals" upperBound="-1"
          eType="#//port/Canal" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PartialDistance">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="distance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="port" eType="#//port/Port"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="cargo" nsURI="http://com.mmxlabs.lng.emf/cargo" nsPrefix="com.mmxlabs.lng.emf.cargo">
    <eClassifiers xsi:type="ecore:EClass" name="CargoModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="cargoes" upperBound="-1"
          eType="#//cargo/Cargo" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Cargo">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="loadSlot" eType="#//cargo/LoadSlot"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="dischargeSlot" eType="#//cargo/DischargeSlot"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LoadSlot">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minQuantity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxQuantity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="unitPrice" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="port" eType="#//port/Port"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="windowStart" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="windowDuration" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DischargeSlot">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minQuantity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxQuantity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="unitPrice" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="port" eType="#//port/Port"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="windowStart" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="windowDuration" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="contract" nsURI="http://com.mmxlabs.lng.emf/contract" nsPrefix="com.mmxlabs.lng.emf.contract">
    <eClassifiers xsi:type="ecore:EClass" name="ContractModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="purchaseContracts" upperBound="-1"
          eType="#//contract/PurchaseContract" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="salesContracts" upperBound="-1"
          eType="#//contract/SalesContract" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PurchaseContract"/>
    <eClassifiers xsi:type="ecore:EClass" name="SalesContract"/>
  </eSubpackages>
  <eSubpackages name="market" nsURI="http://com.mmxlabs.lng.emf/market" nsPrefix="com.mmxlabs.lng.emf.market">
    <eClassifiers xsi:type="ecore:EClass" name="MarketModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="markets" upperBound="-1"
          eType="#//market/Market" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Market">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="forwardPriceCurve" upperBound="-1"
          eType="#//market/ForwardPrice" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ForwardPrice">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="date" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="price" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="optimiser" nsURI="http://com.mmxlabs.lng.emf/optimiser" nsPrefix="com.mmxlabs.lng.emf.optimiser">
    <eClassifiers xsi:type="ecore:EClass" name="OptimisationSettings">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Optimisation">
      <eStructuralFeatures xsi:type="ecore:EReference" name="allSettings" eType="#//optimiser/OptimisationSettings"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="currentSettings" eType="#//optimiser/OptimisationSettings"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
