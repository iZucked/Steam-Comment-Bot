<plugin>

   <extension
         point="org.eclipse.core.runtime.applications"
         id="LngEditorAdvisorApplication">
      <application>
         <run class="scenario.presentation.LngEditorAdvisor$Application"/>
      </application>
   </extension>

   <extension point="org.eclipse.ui.perspectives">
      <perspective
            name="%_UI_Perspective_label"
            class="scenario.presentation.LngEditorAdvisor$Perspective"
            id="scenario.presentation.LngEditorAdvisorPerspective">
      </perspective>
   </extension>

   <extension point="org.eclipse.ui.commands">
      <command
            name="%_UI_Menu_OpenURI_label"
            description="%_UI_Menu_OpenURI_description"
            categoryId="org.eclipse.ui.category.file"
            id="scenario.presentation.LngEditorAdvisorOpenURICommand"/>
      <command
            name="%_UI_Menu_Open_label"
            description="%_UI_Menu_Open_description"
            categoryId="org.eclipse.ui.category.file"
            id="scenario.presentation.LngEditorAdvisorOpenCommand"/>
   </extension>

   <extension point="org.eclipse.ui.bindings">
      <key
            commandId="scenario.presentation.LngEditorAdvisorOpenURICommand"
            sequence="M1+U"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="scenario.presentation.LngEditorAdvisorOpenCommand"
            sequence="M1+O"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
   </extension>

   <extension point="org.eclipse.ui.actionSets">
      <actionSet
            label="%_UI_LngEditorAdvisor_ActionSet_label"
            visible="true"
            id="LngEditorAdvisorActionSet">
         <action
               label="%_UI_Menu_About_label"
               class="scenario.presentation.LngEditorAdvisor$AboutAction"
               menubarPath="help/additions"
               id="scenario.presentation.LngEditorAdvisorAboutAction"/>
         <action
               label="%_UI_Menu_OpenURI_label"
               definitionId="scenario.presentation.LngEditorAdvisorOpenURICommand"
               class="scenario.presentation.LngEditorAdvisor$OpenURIAction"
               menubarPath="file/additions"
               id="scenario.presentation.LngEditorAdvisorOpenURIAction"/>
         <action
               label="%_UI_Menu_Open_label"
               definitionId="scenario.presentation.LngEditorAdvisorOpenCommand"
               class="scenario.presentation.LngEditorAdvisor$OpenAction"
               menubarPath="file/additions"
               id="scenario.presentation.LngEditorAdvisorOpenAction"/>
      </actionSet>
      <actionSet
            id="com.mmxlabs.lngscheduler.emf.editor.actionSet"
            label="Scenario Actions"
            visible="true">
         <action
               class="com.mmxlabs.lngscheduler.emf.editor.actions.PriceCurveAction"
               icon="icons/curve.gif"
               id="com.mmxlabs.lngscheduler.emf.editor.actions.PriceCurveAction"
               label="Edit Default &amp;Discount Curve"
               menubarPath="scenarioMenuID/actions"
               toolbarPath="scenarioGroup"
               tooltip="Edit the active discount curve for this scenario">
         </action>
         <action
               class="com.mmxlabs.lngscheduler.emf.editor.actions.OptimisationSettingsAction"
               icon="icons/settings.gif"
               id="com.mmxlabs.lngscheduler.emf.editor.actions.OptimiserSettingsAction"
               label="Edit Optimisation Settings"
               menubarPath="scenarioMenuID/actions"
               toolbarPath="scenarioGroup"
               tooltip="Edit the optimisation settings for this scenario">
         </action>
         <action
               class="com.mmxlabs.lngscheduler.emf.editor.actions.ClearSolutionsAction"
               icon="icons/clear.gif"
               id="com.mmxlabs.lngscheduler.emf.editor.actions.ClearSolutionsIcon"
               label="Remove Solutions and Vessel Assignments"
               menubarPath="scenarioMenuID/actions"
               toolbarPath="scenarioGroup"
               tooltip="Removes all solutions and vessel assignments from this scenario">
         </action>
         <!--
         <action
               class="com.mmxlabs.lngscheduler.emf.editor.actions.LinkStaticDataAction"
               id="com.mmxlabs.lngscheduler.emf.editor.actions.LinkStaticDataAction"
               label="Link to Static Data"
               menubarPath="scenarioMenuID/actions"
               toolbarPath="scenarioGroup"
               tooltip="Links the non-static data in this scenario to a static data set">
         </action>
         <action
               class="com.mmxlabs.lngscheduler.emf.editor.actions.ExtractStaticDataAction"
               id="com.mmxlabs.lngscheduler.emf.editor.actions.ExtractStaticDataAction"
               label="Extract Static Data"
               menubarPath="scenarioMenuID/actions"
               toolbarPath="scenarioGroup"
               tooltip="Extract the static data from this scenario into a separate model">
         </action>
         -->
      </actionSet>
   </extension>

   <extension point="org.eclipse.ui.actionSets">
      <actionSet
            label="%_UI_ScenarioModelWizard_ActionSet_label"
            visible="true"
            id="scenario.presentation.ScenarioActionBarContributorActionSet">
         <action
               label="%_UI_ScenarioModelWizard_label"
               class="scenario.presentation.ScenarioActionBarContributor$NewAction"
               menubarPath="file/new/additions"
               id="scenario.presentation.ScenarioActionBarContributorNewAction"/>
      </actionSet>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="scenario.presentation.ScenarioEditorID"
            name="%_UI_ScenarioEditor_label"
            icon="icons/full/obj16/ScenarioModelFile.gif"
            extensions="scenario,portmodel"
            class="scenario.presentation.ScenarioEditor"
            contributorClass="scenario.presentation.ScenarioActionBarContributor">
      </editor>
   </extension>

