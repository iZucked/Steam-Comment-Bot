   <extension
         point="org.eclipse.ui.handlers">
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="com.mmxlabs.lngscheduler.emf.editor.importer.sampleCategory"
            name="Scenario Import">
      </category>
      <wizard
            category="com.mmxlabs.lngscheduler.emf.editor.importer.sampleCategory"
            class="com.mmxlabs.lngscheduler.emf.editor.wizards.CSVImportWizard"
            icon="icons/sample.gif"
            id="com.mmxlabs.lngscheduler.emf.editor.importer.CSVImportWizard"
            name="Import Scenario from CSV Files">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
      <wizard
            category="com.mmxlabs.lngscheduler.emf.editor.importer.sampleCategory"
            class="com.mmxlabs.lngscheduler.emf.editor.wizards.PortModelCSVImportWizard"
            icon="icons/sample.gif"
            id="com.mmxlabs.lngscheduler.emf.editor.importer.PortModelCSVImportWizard"
            name="Import Port Model from CSV Files">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="com.mmxlabs.lngscheduler.emf.editor"
            name="Scenario Creation">
      </category>
      <wizard
            category="com.mmxlabs.lngscheduler.emf.editor"
            class="com.mmxlabs.lngscheduler.emf.editor.wizards.DerivedScenarioWizard"
            icon="icons/sample.gif"
            id="com.mmxlabs.lngscheduler.emf.editor.wizards.DerivedScenarioWizard"
            name="Create Derived Scenario">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="com.mmxlabs.lngscheduler.emf.editor.export"
            name="Scenario Export">
      </category>
      <wizard
            category="com.mmxlabs.lngscheduler.emf.editor.export"
            class="com.mmxlabs.lngscheduler.emf.editor.wizards.CSVExportWizard"
            id="com.mmxlabs.lngscheduler.emf.editor.export-csv"
            name="Export Scenario to CSV Files">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="test.TestEditorPart"
            id="test.TestEditorPart"
            name="est Editor">
      </view>
   </extension>

</plugin>
