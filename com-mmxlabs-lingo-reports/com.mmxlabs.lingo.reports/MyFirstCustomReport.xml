<?eclipse version="3.4"?>
<plugin>
    <extension
          point="com.mmxlabs.lingo.reports.ScheduleBasedReportInitialState">
       <ScheduleBasedReportInitialState
             customisable="false"
             id="com.mmxlabs.lingo.reports.MyFirstCustomReport">
         <InitialRowType
               rowType="cargo">
         </InitialRowType>
         <InitialRowType
               rowType="long">
         </InitialRowType>
         <InitialRowType
               rowType="short">
         </InitialRowType>
         <InitialRowType
               rowType="virtualcharters">
         </InitialRowType>
         <InitialRowType
               rowType="event">
         </InitialRowType>
         <InitialRowType
               rowType="orphanlegs">
         </InitialRowType>
         <InitialRowType
               rowType="endevents">
         </InitialRowType>
         <InitialRowType
               rowType="charterlength">
         </InitialRowType>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.schedule">
         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.l-id">
         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.d-id">
         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.type">
         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.vessel">
         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.pnl_total">
         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.pnl_additional">
         </InitialColumn>
         <InitialColumn
