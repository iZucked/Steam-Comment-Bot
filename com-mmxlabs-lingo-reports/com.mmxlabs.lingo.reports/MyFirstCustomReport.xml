         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.buyvolume_m3">
         </InitialColumn>
         <InitialColumn
               id="com.mmxlabs.lingo.reports.components.columns.schedule.sellvolume_m3">
         </InitialColumn>
         <InitialDiffOption
               option="scenario">
         </InitialDiffOption>
         <InitialDiffOption
               option="vessel">
         </InitialDiffOption>
       </ScheduleBasedReportInitialState>
    </extension>
    <extension
          point="org.eclipse.ui.views">
       <view
             category="com.mmxlabs.shiplingo.platform.reports"
             class="org.ops4j.peaberry.eclipse.GuiceExtensionFactory:com.mmxlabs.lingo.reports.views.schedule.ScheduleSummaryReport"
             icon="icons/cview16/exec_statistic_view.gif"
             id="com.mmxlabs.lingo.reports.MyFirstCustomReport"
             name="My First custom report!"
             restorable="false">
       </view>
    </extension>
 </plugin>