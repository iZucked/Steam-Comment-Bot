<?eclipse version="3.4"?>
<plugin>
   <extension-point id="ParameterMode" name="ParameterMode" schema="schema/ParameterMode.exsd"/>
   <extension-point id="ParameterModeExtender" name="ParameterModeExtender" schema="schema/ParameterModeExtender.exsd"/>
	<extension
            point="org.eclipse.ui.handlers">
 		<handler
            class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.ForkScenarioCommandHandler"
            commandId="com.mmxlabs.scenario.service.ui.fork">
         <activeWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="com.mmxlabs.scenario.service.ui.caps.forking">
                  </test>
               </iterate>
            </with>
         </activeWhen>
      </handler>            
         <handler
               class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.StartOptimisationHandler"
               commandId="com.mmxlabs.commands.optimisation.play">
            <enabledWhen>
                  <with
                        variable="selection">
               <iterate
                     ifEmpty="false">
                  <and>
                     <instanceof
                           value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                     </instanceof>
                     <not><test
                          property="com.mmxlabs.scenario.service.ui.flags.readonly"
                          forcePluginActivation="true">
                     </test></not>
                     <test
                           forcePluginActivation="true"
                           property="com.mmxlabs.shiplingo.platform.models.optimisation.properties.canPlay">
                     </test>
                  </and>
               </iterate>
                  </with>
            </enabledWhen>
            <activeWhen>
              <with
                        variable="selection">
               <iterate
                        ifEmpty="false">
                  <and>
                     <instanceof
                           value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                     </instanceof>
                  </and>
               </iterate>
                    </with>
            </activeWhen>
         </handler>
         <handler
               class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.StopOptimisationHandler"
               commandId="com.mmxlabs.commands.optimisation.terminate">
            <enabledWhen>
               <iterate
                     ifEmpty="false">
                  <and>
                     <instanceof
                           value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                     </instanceof>
                                         <not><test
                          property="com.mmxlabs.scenario.service.ui.flags.readonly"
                          forcePluginActivation="true">
                     </test></not>
                     <test
                           property="com.mmxlabs.shiplingo.platform.models.optimisation.properties.canTerminate">
                     </test>
                  </and>
               </iterate>
            </enabledWhen>
            <activeWhen>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false">
                     <and>
                        <instanceof
                              value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                        </instanceof>
                     </and>
                  </iterate>
               </with>
            </activeWhen>
         </handler>
         <handler
               class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.EvaluateOptimisationHandler"
               commandId="com.mmxlabs.commands.optimisation.evaluate">
            <enabledWhen>
               <iterate
                     ifEmpty="false">
                  <and>
                     <instanceof
                           value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                     </instanceof>
                       <not><test
                          property="com.mmxlabs.scenario.service.ui.flags.readonly"
                          forcePluginActivation="true">
                     </test></not>
                     <not>
                        <test
                              property="com.mmxlabs.shiplingo.platform.models.optimisation.properties.hasActiveJob">
                        </test>
                     </not>
                  </and>
               </iterate>
            </enabledWhen>
            <activeWhen>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false">
                     <and>
                        <instanceof
                              value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                        </instanceof>
                     </and>
                  </iterate>
               </with>
            </activeWhen>
         </handler>
      </extension>

	<extension
         point="org.eclipse.ui.commands">
         <category
            id="com.mmxlabs.shiplingo.platform.models.manifest.optimisation.commands.category"
            name="Optimisation Commands">
      </category>
   
      <command
            categoryId="com.mmxlabs.shiplingo.platform.models.manifest.optimisation.commands.category"
            id="com.mmxlabs.shiplingo.platform.models.manifest.optimisation.commands.SaveOptimisationJob"
            name="Save Optimisation Job">
      </command>
   </extension>
   
   <extension
         id="toolbar:com.mmxlabs.jobcontroller.views.JobManager"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:com.mmxlabs.jobcontroller.views.JobManager">
         <command
               commandId="com.mmxlabs.shiplingo.platform.models.manifest.optimisation.commands.SaveOptimisationJob"
               disabledIcon="icons/dtool16/saveas_edit.gif"
               icon="icons/etool16/saveas_edit.gif"
               label="Save"
               style="push"
               tooltip="Save selected optimisation">
         </command>
      </menuContribution>
      
      
   </extension>
   
         <extension
            point="org.eclipse.core.runtime.adapters">
         <factory
               adaptableType="com.mmxlabs.models.lng.transformer.ui.LNGSchedulerJobDescriptor"
               class="com.mmxlabs.models.lng.transformer.ui.adapterfactories.LNGJobControlAdapterFactory">
            <adapter
                  type="com.mmxlabs.jobmanager.jobs.IJobControl">
            </adapter>
         </factory>
         <factory
               adaptableType="com.mmxlabs.models.lng.transformer.ui.LNGRunAllSimilarityJobDescriptor"
               class="com.mmxlabs.models.lng.transformer.ui.adapterfactories.LNGJobControlAdapterFactory">
            <adapter
                  type="com.mmxlabs.jobmanager.jobs.IJobControl">
            </adapter>
         </factory>
         <factory
               adaptableType="com.mmxlabs.models.lng.transformer.ui.AbstractLNGJobDescriptor"
               class="com.mmxlabs.models.lng.transformer.ui.adapterfactories.LNGJobControlAdapterFactory">
            <adapter
                  type="com.mmxlabs.jobmanager.jobs.IJobControl">
            </adapter>
         </factory>
         <factory
               adaptableType="com.mmxlabs.models.lng.transformer.ui.analytics.LNGSlotInsertionJobDescriptor"
               class="com.mmxlabs.models.lng.transformer.ui.adapterfactories.LNGJobControlAdapterFactory">
            <adapter
                  type="com.mmxlabs.jobmanager.jobs.IJobControl">
            </adapter>
         </factory>
         <factory
               adaptableType="com.mmxlabs.models.lng.transformer.ui.actionablesets.CreateActionableSetPlanJobDescriptor"
               class="com.mmxlabs.models.lng.transformer.ui.adapterfactories.LNGJobControlAdapterFactory">
            <adapter
                  type="com.mmxlabs.jobmanager.jobs.IJobControl">
            </adapter>
         </factory>         
         <factory
               adaptableType="com.mmxlabs.models.lng.transformer.ui.analytics.LNGSandboxJobDescriptor"
               class="com.mmxlabs.models.lng.transformer.ui.adapterfactories.LNGJobControlAdapterFactory">
            <adapter
                  type="com.mmxlabs.jobmanager.jobs.IJobControl">
            </adapter>
         </factory>         
      </extension>
   
      <extension
            point="org.eclipse.ui.commands">
         <category
               id="com.mmxlabs.commands.category.optimisation"
               name="Optimisation Commands">
         </category>
         <command
               categoryId="com.mmxlabs.commands.category.optimisation"
               id="com.mmxlabs.commands.optimisation.play"
               name="Start Optimisation">
         </command>
         <command
               categoryId="com.mmxlabs.commands.category.optimisation"
               id="com.mmxlabs.commands.optimisation.cloud"
               name="Start Cloud Optimisation">
         </command>
         <command
               categoryId="com.mmxlabs.commands.category.optimisation"
               id="com.mmxlabs.commands.optimisation.terminate"
               name="Terminate Optimisation">
         </command>
         <category
               id="com.mmxlabs.commands.category.scenario"
               name="Scenario Commands">
         </category>
         <command
               categoryId="com.mmxlabs.commands.category.optimisation"
               id="com.mmxlabs.commands.optimisation.evaluate"
               name="Evaluate Schedule">
         </command>
      </extension>
      
      <extension
            point="org.eclipse.ui.menus">
         <menuContribution
               allPopups="false"
               locationURI="popup:com.mmxlabs.scenario.service.ui.navigator?after=optimisation">
               <command
                     commandId="com.mmxlabs.commands.optimisation.play"
                     icon="platform:/plugin/com.mmxlabs.rcp.common/icons/16x16/optimise.png"
               		 disabledIcon="platform:/plugin/com.mmxlabs.rcp.common/icons/16x16/optimise_disabled.png"
                     id="com.mmxlabs.scenario.service.toolbars.optimisation.play"
                     label="Optimise"
                     style="push"
                     tooltip="Start or resume optimisation">
                  <visibleWhen
                        checkEnabled="false">
                     <and>
                        <with
                              variable="selection">
                           <iterate
                                 ifEmpty="false">
                              <instanceof
                                    value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                              </instanceof>
                                                   <not><test
                          property="com.mmxlabs.scenario.service.ui.flags.readonly"
                          forcePluginActivation="true">
                     </test></not>

                           </iterate>
                        </with>
                        <count
                              value="1">
                        </count>
                     </and>
                  </visibleWhen>
               </command>
         </menuContribution>
      </extension>
      <extension
            point="org.eclipse.core.expressions.propertyTesters">
         <propertyTester
               class="com.mmxlabs.models.lng.transformer.ui.propertytesters.JobStatePropertyTester"
               id="com.mmxlabs.shiplingo.platform.models.optimisation.propertyTester"
               namespace="com.mmxlabs.shiplingo.platform.models.optimisation.properties"
               properties="jobState,canPlay,canTerminate,hasActiveJob"
               type="com.mmxlabs.scenario.service.model.ScenarioInstance">
         </propertyTester>
      </extension>
      <extension
            point="org.eclipse.ui.editorActions">
         <editorContribution
               id="com.mmxlabs.shiplingo.platform.models.optimisation.editorContributions"
               targetID="com.mmxlabs.models.lng.demo.scneditor">
            <action
                  class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.editor.StopOptimisationEditorActionDelegate"
                  definitionId="com.mmxlabs.commands.optimisation.terminate"
                  icon="platform:/plugin/com.mmxlabs.rcp.common/icons/24x24/terminate.png"
                  disabledIcon="platform:/plugin/com.mmxlabs.rcp.common/icons/24x24/terminate_disabled.png"
                  id="com.mmxlabs.shiplingo.platform.models.optimisation.stop"
                  label="Stop Optimisation"
                  style="push"
                  toolbarPath="optimisationgroup"
                  tooltip="Stop optimisation">
            </action>
            <action
                  class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.editor.StartOptimisationEditorActionDelegate"
                  definitionId="com.mmxlabs.commands.optimisation.play"
                  icon="platform:/plugin/com.mmxlabs.rcp.common/icons/24x24/optimise.png"
                  disabledIcon="platform:/plugin/com.mmxlabs.rcp.common/icons/24x24/optimise_disabled.png"
                  id="com.mmxlabs.shiplingo.platform.models.optimisation.play"
                  label="Start Optimisation"
                  toolbarPath="optimisationgroup"
                  tooltip="Start an optimisation">
            </action>
            <action
                  class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.editor.EvaluateOptimisationEditorActionDelegate"
                  definitionId="com.mmxlabs.commands.optimisation.evaluate"
                  icon="icons/evaluate_schedule.png"
                  disabledIcon="icons/evaluate_schedule_disabled.png"
                  id="com.mmxlabs.shiplingo.platform.models.optimisation.action3"
                  label="Evaluate"
                  toolbarPath="evaluategroup3"
                  tooltip="Evaluate scenario">
            </action>
            <action
                  class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.editor.LiveEvaluatorStateEditorActionDelegate"
                  icon="icons/auto_evaluate.png"
                  disabledIcon="icons/auto_evaluate_disabled.png"
                  id="com.mmxlabs.scenario.service.ui.autoeval"
                  label="Auto-evaluate on/off"
                  style="toggle"
                  toolbarPath="autoevaluategroup">
            </action>
            <action
                  class="com.mmxlabs.models.lng.transformer.ui.navigator.handlers.editor.ForkScenarioEditorActionDelegate"
                  definitionId="com.mmxlabs.scenario.service.ui.fork"
                  icon="platform:/plugin/com.mmxlabs.rcp.common/icons/24x24/fork.png"
                  disabledIcon="platform:/plugin/com.mmxlabs.rcp.common/icons/24x24/fork_disabled.png"
                  id="com.mmxlabs.scenario.service.ui.fork"
                  label="Fork"
                  style="push"
                  toolbarPath="forkgroup"
                  tooltip="Fork scenario and continue editing">
            </action>
         </editorContribution>
      </extension>
      
      <extension
         point="org.eclipse.emf.validation.constraintProviders">
      <category
            id="com.mmxlabs.models.lng.transformer.ui.validation.base"
            mandatory="true"
            name="Transformer UI Validation">
      </category>
      <constraintProvider
            cache="true">
         <package
               namespaceUri="http://www.mmxlabs.com/models/lng/cargo/1/">
         </package>
         <constraints
               categories="com.mmxlabs.trading.integration.validation.base">
            <constraint
                  class="com.mmxlabs.models.lng.transformer.ui.validation.SingleMissingPriceConstraint"
                  id="com.mmxlabs.models.lng.transformer.ui.validation.SingleMissingPriceConstraint"
                  isEnabledByDefault="true"
                  lang="Java"
                  mode="Batch"
                  name="Missing Price Constraint"
                  severity="ERROR"
                  statusCode="1">
               <message>
                  {0}
               </message>
               <target
                     class="Cargo">
               </target>
            </constraint>
            <constraint
                  class="com.mmxlabs.models.lng.transformer.ui.validation.BreakEvenCharterOutConstraint"
                  id="com.mmxlabs.models.lng.transformer.ui.validation.BreakEvenCharterOutConstraint"
                  isEnabledByDefault="true"
                  lang="Java"
                  mode="Batch"
                  name="Break Even &amp; Charter Out Constraint"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  {0}
               </message>
               <target
                     class="Slot">
               </target>
            </constraint>
         </constraints>
      </constraintProvider>
   </extension>
      <extension
            point="com.mmxlabs.license.features.PluginXMLEnablement">
         <PluginXMLEnablement
