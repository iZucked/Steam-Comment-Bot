         <PluginXMLEnablement
               feature="charter-length"
               plugin.xml="charter-length.plugin.xml">
         </PluginXMLEnablement>    
         <PluginXMLEnablement
               feature="mtm-action"
               plugin.xml="mtm-action.plugin.xml">
         </PluginXMLEnablement>   
         <PluginXMLEnablement
               feature="generated-papers"
               plugin.xml="generated-papers-in-pnl.plugin.xml">
         </PluginXMLEnablement>
         <PluginXMLEnablement
               feature="load-trigger"
               plugin.xml="load-trigger.plugin.xml">
         </PluginXMLEnablement>  
         <PluginXMLEnablement
               feature="anonymise-scenario"
               plugin.xml="anonymise-scenario.plugin.xml">
         </PluginXMLEnablement>        
      </extension>
      <extension
            point="org.eclipse.ui.startup">
         <startup
               class="com.mmxlabs.models.lng.transformer.ui.internal.PluginStartup">
         </startup>
      </extension>
      
</plugin>
