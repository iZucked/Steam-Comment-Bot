<?eclipse version="3.4"?>
<plugin>

   <extension
         id="com.mmxlabs.lingo.app.headless.HeadlessApplication"
         name="Headless Scenario Runner"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="*"
            thread="main"
            visible="true">
         <run
               class="com.mmxlabs.lingo.app.headless.HeadlessApplication">
         </run>
      </application>
   </extension>
   <extension
         id="com.mmxlabs.lingo.app.headless.HeadlessOptioniser"
         name="Headless Optioniser"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.mmxlabs.lingo.app.headless.HeadlessOptioniserApplication">
         </run>
      </application>
   </extension>
</plugin>
