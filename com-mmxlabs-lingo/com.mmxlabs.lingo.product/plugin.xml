<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.mmxlabs.lingo.app.application"
            name="LiNGO">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif">
         </property>
         <property
               name="appName"
               value="LiNGO">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="20,220,440,20">
         </property>
         <property
               name="startupProgressRect"
               value="20,210,440,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="aboutImage"
               value="icons/alt_window_16.gif">
         </property>
	 <property
        name="aboutText"
        value="LiNGO Platform

Version 2.4.2">
       </property>
      </product>
   </extension>
   <extension
	       point="org.eclipse.ui.activities">
	    <activity
	          id="com.mmxlabs.lingo.default.activity"
	          name="Default App Activity">
	    </activity>
	    <activityPatternBinding
	          activityId="com.mmxlabs.lingo.default.activity"
	          isEqualityPattern="false"
	          pattern=".*">
	    </activityPatternBinding>
   </extension>
   <extension
         point="com.mmxlabs.models.migration.DefaultMigrationContext">
      <DefaultMigrationContext
            context="com.mmxlabs.lingo">
      </DefaultMigrationContext>
   </extension>
</plugin>
