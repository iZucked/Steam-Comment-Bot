<?eclipse version="3.4"?>
<plugin>
   <extension-point id="DataServerEndPoint" name="DataServerEndPoint" schema="schema/DataServerEndPoint.exsd"/>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.mmxlabs.lngdataserver.server.preferences.DataserverPreferencePage"
            id="com.mmxlabs.lngdataserver.server.DataserverPreferencePage"
            name="Data Hub">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.mmxlabs.lngdataserver.server.preferences.DatahubPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.mmxlabs.lngdataserver.server.internal.PluginStartup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.mmxlabs.lngdataserver.server.internal.HubPermissionsFlagsPropertyTester"
            id="com.mmxlabs.lngdataserver.server.HubPermissionsFlagsPropertyTester"
            namespace="com.mmxlabs.lngdataserver.server.permissions"
            properties="hubsupports,ispermitted"
            type="java.lang.Object">
      </propertyTester>
   </extension>

</plugin>
