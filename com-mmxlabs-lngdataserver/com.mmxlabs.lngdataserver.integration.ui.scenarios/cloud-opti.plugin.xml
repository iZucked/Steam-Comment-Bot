<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.view.CloudManagerView"
            icon="platform:/plugin/com.mmxlabs.rcp.common/icons/16x16/cloud.png"
            id="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.view.CloudManagerView"
            name="Cloud Manager"
            restorable="true">
      </view>
   </extension>
   
   
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.preferences.CloudOptimiserPreferencePage"
            id="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.preferences.CloudOptimiserPreferencePage"
            name="Cloud Optimiser">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.preferences.CloudOptimiserPreferenceInitializer">
      </initializer>
   </extension>
   
</plugin>
