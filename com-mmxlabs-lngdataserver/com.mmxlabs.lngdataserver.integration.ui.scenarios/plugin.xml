<plugin>
   <extension-point id="ReportPublisher" name="ReportPublisher" schema="schema/ReportPublisher.exsd"/>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.mmxlabs.lngdataserver.integration.ui.scenarios.internal.PluginStartup">
      </startup>
   </extension>
   <extension
         id="com.mmxlabs.hub.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi"
            apply="always">
      </fragment>
   </extension>
 	<extension
            point="com.mmxlabs.license.features.PluginXMLEnablement">
         <PluginXMLEnablement
               feature="cloud-optimisation"
               plugin.xml="cloud-opti.plugin.xml">
         </PluginXMLEnablement>
      </extension>   
         <extension
         point="org.eclipse.ui.views">
      <view
            class="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.view.CloudManagerView"
            icon="icons:/16/cloudplay"
            id="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.view.TaskManagerView"
            name="Run Manager"
            restorable="true">
      </view>
   </extension>
         <extension
               point="com.mmxlabs.rcp.common.ViewPartRewrite">
            <ViewPartRewrite
                  newViewID="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.view.TaskManagerView"
                  oldViewID="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.view.CloudManagerView">
            </ViewPartRewrite>
         </extension>
         <extension
               point="com.mmxlabs.rcp.common.DebugOptionsProvider">
            <DebugOptionsProvider
                  cls="com.mmxlabs.lngdataserver.integration.ui.scenarios.cloud.debug.CloudOptiDebugOptionsProvider">
            </DebugOptionsProvider>
         </extension>
   
   
</plugin>
