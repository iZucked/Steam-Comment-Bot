<plugin>
 
   <!-- DISTANCES -->
     <extension
           point="org.eclipse.ui.menus">
         
         <menuContribution
               allPopups="false"
               locationURI="menu:data?after=dataStart">
            <command
                  commandId="com.mmxlabs.models.lng.scenario.import.wizards.distances"
                  label="Import distances"
                  style="push">
            </command>
         </menuContribution>
     </extension>
      <extension
            point="org.eclipse.ui.commands">
         <command
               defaultHandler="com.mmxlabs.lngdataserver.lng.importers.distances.ui.ImportDistancesWizardHandler"
               id="com.mmxlabs.models.lng.scenario.import.wizards.distances"
               name="Copy ports and distances to scenario">
         </command>       
      </extension>
      
      <!-- PRICES -->
	<extension
           point="org.eclipse.ui.menus">
         
         <menuContribution
               allPopups="false"
               locationURI="menu:data?after=dataStart">
            <command
                  commandId="com.mmxlabs.models.lng.scenario.import.wizards.prices"
                  label="Import prices"
                  style="push">
            </command>
            <command
                  commandId="com.mmxlabs.models.lng.scenario.export.wizards.prices"
                  label="Export prices"
                  style="push">
            </command>
         </menuContribution>
     </extension>
      <extension
            point="org.eclipse.ui.commands">
         <command
               defaultHandler="com.mmxlabs.lngdataserver.lng.importers.pricing.ui.ImportPricingWizardHandler"
               id="com.mmxlabs.models.lng.scenario.import.wizards.prices"
               name="Copy prices to scenario">
         </command>
         <command
               defaultHandler="com.mmxlabs.lngdataserver.lng.exporters.pricing.ui.ExportPricingWizardHandler"
               id="com.mmxlabs.models.lng.scenario.export.wizards.prices"
               name="Copy prices from scenario">
         </command>
      </extension>
      <extension
            point="org.eclipse.ui.commands">
         <command
               defaultHandler="com.mmxlabs.lngdataserver.lng.importers.vessels.ui.ImportVesselsWizardHandler"
               id="com.mmxlabs.models.lng.scenario.import.wizards.vessels"
               name="Copy vessels to scenario">
         </command>
      </extension>
      <extension
            point="org.eclipse.ui.menus">
         <menuContribution
               allPopups="false"
               locationURI="menu:data?after=dataStart">
            <command
                  commandId="com.mmxlabs.models.lng.scenario.import.wizards.vessels"
                  label="Import vessels"
                  style="push">
            </command>
         </menuContribution>
      </extension>
      <extension
            point="org.eclipse.ui.commands">
         <command
               defaultHandler="com.mmxlabs.lngdataserver.lng.importers.port.ui.ImportPortsWizardHandler"
               id="com.mmxlabs.models.lng.scenario.import.wizards.port"
               name="Copy ports to scenario">
         </command>
         <command
               defaultHandler="com.mmxlabs.lngdataserver.lng.exporters.port.ui.ExportPortWizardHandler"
               id="com.mmxlabs.models.lng.scenario.export.wizards.port"
               name="Copy ports from scenario">
         </command>
      </extension>
      <extension
            point="org.eclipse.ui.menus">
         <menuContribution
               allPopups="false"
               locationURI="menu:data?after=dataStart">
            <command
                  commandId="com.mmxlabs.models.lng.scenario.import.wizards.port"
                  label="Import ports"
                  style="push">
            </command>
            <command
                  commandId="com.mmxlabs.models.lng.scenario.export.wizards.port"
                  label="Export ports"
                  style="push">
            </command>
         </menuContribution>
      </extension>
      <extension point="com.mmxlabs.lngdataserver.browser.ui.DataBrowserContextMenuExtension">
         <DataBrowserContextMenuExtension
               class="com.mmxlabs.lngdataserver.lng.importers.menus.DataBrowserContentMenuContribution">
         </DataBrowserContextMenuExtension>
      </extension>
</plugin>
