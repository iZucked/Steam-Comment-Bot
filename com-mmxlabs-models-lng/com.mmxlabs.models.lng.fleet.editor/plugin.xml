<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.mmxlabs.models.lng.fleet.presentation.FleetModelWizardID"
            name="%_UI_FleetModelWizard_label"
            class="com.mmxlabs.models.lng.fleet.presentation.FleetModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/FleetModelFile.gif">
         <description>%_UI_FleetModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="com.mmxlabs.models.lng.fleet.presentation.FleetEditorID"
            name="%_UI_FleetEditor_label"
            icon="icons/full/obj16/FleetModelFile.gif"
            extensions="fleet"
            class="com.mmxlabs.models.lng.fleet.presentation.FleetEditor"
            contributorClass="com.mmxlabs.models.lng.fleet.presentation.FleetActionBarContributor">
      </editor>
   </extension>

	
	<extension
		point="com.mmxlabs.models.ui.componenthelpers">
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.Vessel"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.VesselComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.Vessel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.VesselClass"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.VesselClassComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.VesselClass" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.VesselEvent"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.VesselEventComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.VesselEvent" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.FleetModel"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.FleetModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.FleetModel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.BaseFuel"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.BaseFuelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.BaseFuel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.DryDockEvent"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.DryDockEventComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.DryDockEvent" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.CharterOutEvent"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.CharterOutEventComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.CharterOutEvent" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.HeelOptions"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.HeelOptionsComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.HeelOptions" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.VesselStateAttributes"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.VesselStateAttributesComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.VesselStateAttributes" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.VesselAvailiability"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.VesselAvailabilityComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.VesselAvailability" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.fleet.helpers.FuelConsumption"
			helperClass="com.mmxlabs.models.lng.fleet.presentation.composites.FuelConsumptionComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.fleet.FuelConsumption" />
		
	</extension>
 <extension
       point="com.mmxlabs.models.ui.valueproviders">
    <valueProvider
          factory="com.mmxlabs.models.lng.fleet.ui.valueproviders.VesselValueProviderFactory"
          id="com.mmxlabs.models.lng.fleet.editor.fleetValueProvider"
          referenceEClass="com.mmxlabs.models.lng.types.AVesselSet">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.fleet.ui.valueproviders.BaseFuelValueProviderFactory"
          id="com.mmxlabs.models.lng.fleet.editor.baseFuelProvider"
          referenceEClass="com.mmxlabs.models.lng.types.ABaseFuel">
    </valueProvider>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.jointmodeleditors">
    <jointModelEditor
          id="com.mmxlabs.models.lng.fleet.editor.jointModelEditorContribution"
          implementation="com.mmxlabs.models.lng.fleet.ui.editorpart.FleetModelEditorContribution"
          priority="3"
          subModelClass="com.mmxlabs.models.lng.fleet.FleetModel">
    </jointModelEditor>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.submodels">
    <submodelImporter
          ID="com.mmxlabs.models.lng.fleet.editor.fleetModelImporter"
          subModelClassName="com.mmxlabs.models.lng.fleet.FleetModel"
          subModelImporterClass="com.mmxlabs.models.lng.fleet.importer.FleetModelImporter">
    </submodelImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.displaycompositefactories">
    <displayCompositeFactory
          eClass="com.mmxlabs.models.lng.fleet.VesselStateAttributes"
          factory="com.mmxlabs.models.lng.fleet.ui.displaycomposites.VSADisplayCompositeFactory"
          id="com.mmxlabs.models.lng.fleet.editor.attributesDisplayCompositeFactory"
          inheritable="true">
    </displayCompositeFactory>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.modelfactories">
    <factory
          ID="com.mmxlabs.models.lng.fleet.editor.vesselClassFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Vessel Class"
          targetEClass="com.mmxlabs.models.lng.fleet.VesselClass">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.fleet.editor.vesselFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Vessel"
          targetEClass="com.mmxlabs.models.lng.fleet.Vessel">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.fleet.editor.charterOutFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Charter Out"
          outputEClass="com.mmxlabs.models.lng.fleet.CharterOutEvent"
          targetEClass="com.mmxlabs.models.lng.fleet.VesselEvent">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.fleet.editor.dryDockFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Dry Dock"
          outputEClass="com.mmxlabs.models.lng.fleet.DryDockEvent"
          targetEClass="com.mmxlabs.models.lng.fleet.VesselEvent">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.fleet.editor.maintenanceFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Maintenance"
          outputEClass="com.mmxlabs.models.lng.fleet.MaintenanceEvent"
          targetEClass="com.mmxlabs.models.lng.fleet.VesselEvent">
    </factory>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.classes">
    <classImporter
          ID="com.mmxlabs.models.lng.fleet.editor.vesselClassImporter"
          classImporterClass="com.mmxlabs.models.lng.fleet.importer.VesselClassImporter"
          eClassName="com.mmxlabs.models.lng.fleet.VesselClass"
          inheritable="true">
    </classImporter>
    <classImporter
          ID="com.mmxlabs.models.lng.fleet.editor.baseFuelImporter"
          classImporterClass="com.mmxlabs.models.lng.fleet.importer.BaseFuelImporter"
          eClassName="com.mmxlabs.models.lng.fleet.BaseFuel"
          inheritable="true">
    </classImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.editorfactories">
    <editorFactory
          factoryClass="com.mmxlabs.models.lng.fleet.ui.inlineeditors.RouteCostInlineEditorFactory"
          id="com.mmxlabs.models.lng.fleet.editor.editorfactories.routeCostInlineEditorFactory">
       <FeatureAnyOf>
          <EClassExtends
                EClass="com.mmxlabs.models.lng.fleet.VesselClassRouteParameters">
          </EClassExtends>
       </FeatureAnyOf>
       <OwnerAnyOf>
          <EClassExtends
                EClass="com.mmxlabs.models.lng.fleet.VesselClass">
          </EClassExtends>
       </OwnerAnyOf>
    </editorFactory>
 </extension>
</plugin>
