<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.emf.validation.constraintProviders">
      <category
            id="com.mmxlabs.models.lng.actuals.validation.base"
            mandatory="true"
            name="Actuals Validation">
      </category>
      <constraintProvider
            cache="true">
         <package
               namespaceUri="http://www.mmxlabs.com/models/lng/actuals/1/">
         </package>
         <constraints
               categories=" com.mmxlabs.models.lng.actuals.validation.base">
            <constraint
                  class="com.mmxlabs.models.lng.actuals.validation.CargoActualsConstraint"
                  id="com.mmxlabs.models.lng.actuals.validation.CargoActualsConstraint"
                  isEnabledByDefault="true"
                  lang="java"
                  mode="Batch"
                  name="Cargo Actuals Constraint"
                  severity="ERROR"
                  statusCode="1">
               <message>
                  {0}
               </message>
               <target
                     class="CargoActuals">
               </target>
            </constraint>
            <constraint
                  class="com.mmxlabs.models.lng.actuals.validation.ActualsSequencingConstraint"
                  id="com.mmxlabs.models.lng.actuals.validation.ActualsSequencingConstraint"
                  isEnabledByDefault="true"
                  lang="java"
                  mode="Batch"
                  name="Actuals Sequence Constraint"
                  severity="ERROR"
                  statusCode="1">
               <message>
                  {0}
               </message>
               <target
                     class="ActualsModel">
               </target>
            </constraint>
         </constraints>
      </constraintProvider>
   </extension>

</plugin>
