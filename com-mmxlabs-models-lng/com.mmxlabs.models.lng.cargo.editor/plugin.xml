       point="com.mmxlabs.models.ui.displaycompositefactories">
    <displayCompositeFactory
          eClass="com.mmxlabs.models.lng.cargo.Cargo"
          factory="com.mmxlabs.models.lng.cargo.ui.displaycomposites.CargoModelCompositeFactory"
          id="com.mmxlabs.models.lng.cargo.editor.cargoCompositeFactory"
          inheritable="true">
    </displayCompositeFactory>
    <displayCompositeFactory
          eClass="com.mmxlabs.models.lng.cargo.LoadSlot"
          factory="com.mmxlabs.models.lng.cargo.ui.displaycomposites.LoadSlotCompositeFactory"
          id="com.mmxlabs.models.lng.cargo.ui.displaycomposites.LoadSlotCompositeFactory"
          inheritable="true">
    </displayCompositeFactory>
    <displayCompositeFactory
          eClass="com.mmxlabs.models.lng.cargo.DischargeSlot"
          factory="com.mmxlabs.models.lng.cargo.ui.displaycomposites.DischargeSlotCompositeFactory"
          id="com.mmxlabs.models.lng.cargo.ui.displaycomposites.DischargeSlotCompositeFactory"
          inheritable="true">
    </displayCompositeFactory>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.submodels">
    <submodelImporter
          ID="com.mmxlabs.models.lng.cargo.editor.cargoModelImporter"
          subModelClassName="com.mmxlabs.models.lng.cargo.CargoModel"
          subModelImporterClass="com.mmxlabs.models.lng.cargo.importer.CargoModelImporter">
    </submodelImporter>
 </extension>
 <extension
       id="com.mmxlabs.models.lng.cargo.import.classes"
       point="com.mmxlabs.models.util.import.classes">
    <classImporter
          ID="com.mmxlabs.models.lng.cargo.editor.CargoImporter"
          classImporterClass="com.mmxlabs.models.lng.cargo.importer.CargoImporter"
          eClassName="com.mmxlabs.models.lng.cargo.Cargo"
          inheritable="true">
    </classImporter>
    <classImporter
          ID="com.mmxlabs.models.lng.cargo.editor.loadSlotImporter"
          classImporterClass="com.mmxlabs.models.lng.cargo.importer.LoadSlotImporter"
          eClassName="com.mmxlabs.models.lng.cargo.LoadSlot"
          inheritable="true">
    </classImporter>
      <classImporter
          ID="com.mmxlabs.models.lng.cargo.editor.dischargeSlotImporter"
          classImporterClass="com.mmxlabs.models.lng.cargo.importer.DischargeSlotImporter"
          eClassName="com.mmxlabs.models.lng.cargo.DischargeSlot"
          inheritable="true">
    </classImporter>
 </extension>
