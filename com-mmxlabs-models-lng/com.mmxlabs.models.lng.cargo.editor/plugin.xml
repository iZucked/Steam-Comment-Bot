<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.mmxlabs.models.lng.cargo.presentation.CargoModelWizardID"
            name="%_UI_CargoModelWizard_label"
            class="com.mmxlabs.models.lng.cargo.presentation.CargoModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CargoModelFile.gif">
         <description>%_UI_CargoModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="com.mmxlabs.models.lng.cargo.presentation.CargoEditorID"
            name="%_UI_CargoEditor_label"
            icon="icons/full/obj16/CargoModelFile.gif"
            extensions="cargo"
            class="com.mmxlabs.models.lng.cargo.presentation.CargoEditor"
            contributorClass="com.mmxlabs.models.lng.cargo.presentation.CargoActionBarContributor">
      </editor>
   </extension>

	
	<extension
		point="com.mmxlabs.models.ui.componenthelpers">
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.Cargo"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.CargoComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.Cargo" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.Slot"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.SlotComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.Slot" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.LoadSlot"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.LoadSlotComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.LoadSlot" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.DischargeSlot"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.DischargeSlotComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.DischargeSlot" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.CargoModel"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.CargoModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.CargoModel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.SpotSlot"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.SpotSlotComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.SpotSlot" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.SpotLoadSlot"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.SpotLoadSlotComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.SpotLoadSlot" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.cargo.helpers.SpotDischargeSlot"
			helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.SpotDischargeSlotComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.cargo.SpotDischargeSlot" />
  <componentHelper
        helperClass="com.mmxlabs.models.lng.cargo.presentation.composites.CargoGroupComponentHelper"
        id="com.mmxlabs.models.lng.cargo.helpers.CargoGroup"
        inheritable="false"
        modelClass="com.mmxlabs.models.lng.cargo.CargoGroup">
  </componentHelper>
		
	</extension>
 <extension
       point="com.mmxlabs.models.ui.jointmodeleditors">
    <jointModelEditor
          id="com.mmxlabs.models.lng.cargo.editor.cargoeditor"
          implementation="com.mmxlabs.models.lng.cargo.ui.editorpart.CargoModelEditorContribution"
          priority="0"
          subModelClass="com.mmxlabs.models.lng.cargo.CargoModel">
    </jointModelEditor>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.valueproviders">
    <valueProvider
          factory="com.mmxlabs.models.lng.cargo.ui.valueproviders.SlotPortValueProviderFactory"
          id="com.mmxlabs.models.lng.cargo.editor.slotRestrictedPortProvider"
          ownerEClass="com.mmxlabs.models.lng.types.ASlot"
          referenceEClass="com.mmxlabs.models.lng.types.APort">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.cargo.ui.valueproviders.SlotContractValueProviderFactory"
          id="com.mmxlabs.models.lng.cargo.editor.slotContractProvider"
          ownerEClass="com.mmxlabs.models.lng.types.ASlot"
          referenceEClass="com.mmxlabs.models.lng.types.AContract">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.cargo.ui.valueproviders.CargoValueProviderFactory"
          id="com.mmxlabs.models.lng.cargo.editor.cargoValueProvider"
          referenceEClass="com.mmxlabs.models.lng.types.ACargo">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.cargo.ui.valueproviders.CargoValueProviderFactory"
          id="com.mmxlabs.models.lng.cargo.editor.cargoGroupValueProvider"
          referenceEClass="com.mmxlabs.models.lng.cargo.CargoGroup">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.cargo.ui.valueproviders.SpotMarketValueProviderFactory"
          id="com.mmxlabs.models.lng.cargo.ui.valueproviders.SpotMarketValueProviderFactory"
          ownerEClass="com.mmxlabs.models.lng.cargo.SpotSlot"
          referenceEClass="com.mmxlabs.models.lng.types.ASpotMarket">
    </valueProvider>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.displaycompositefactories">
    <displayCompositeFactory
          eClass="com.mmxlabs.models.lng.types.ACargo"
          factory="com.mmxlabs.models.lng.cargo.ui.displaycomposites.CargoModelCompositeFactory"
          id="com.mmxlabs.models.lng.cargo.editor.cargoCompositeFactory"
          inheritable="true">
    </displayCompositeFactory>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.submodels">
    <submodelImporter
          ID="com.mmxlabs.models.lng.cargo.editor.cargoModelImporter"
          subModelClassName="com.mmxlabs.models.lng.cargo.CargoModel"
          subModelImporterClass="com.mmxlabs.models.lng.cargo.importer.CargoModelImporter">
    </submodelImporter>
 </extension>
 <extension
       id="com.mmxlabs.models.lng.cargo.import.classes"
       point="com.mmxlabs.models.util.import.classes">
    <classImporter
          ID="com.mmxlabs.models.lng.cargo.editor.CargoImporter"
          classImporterClass="com.mmxlabs.models.lng.cargo.importer.CargoImporter"
          eClassName="com.mmxlabs.models.lng.cargo.Cargo"
          inheritable="true">
    </classImporter>
    <classImporter
          ID="com.mmxlabs.models.lng.cargo.editor.loadSlotImporter"
          classImporterClass="com.mmxlabs.models.lng.cargo.importer.LoadSlotImporter"
          eClassName="com.mmxlabs.models.lng.cargo.LoadSlot"
          inheritable="true">
    </classImporter>
      <classImporter
          ID="com.mmxlabs.models.lng.cargo.editor.dischargeSlotImporter"
          classImporterClass="com.mmxlabs.models.lng.cargo.importer.DischargeSlotImporter"
          eClassName="com.mmxlabs.models.lng.cargo.DischargeSlot"
          inheritable="true">
    </classImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.modelfactories">
    <factory
          ID="com.mmxlabs.models.lng.cargo.editor.cargoFactory"
          factory="com.mmxlabs.models.lng.cargo.ui.modelfactories.CargoFactory"
          label="Cargo"
          targetEClass="com.mmxlabs.models.lng.cargo.Cargo">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.cargo.editor.cargoGroupFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Cargo Group"
          targetEClass="com.mmxlabs.models.lng.cargo.CargoGroup">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.cargo.editor.cargoGroupFactoryForCargoTable"
          factory="com.mmxlabs.models.lng.cargo.ui.modelfactories.CargoGroupFactory"
          label="Cargo Group"
          outputEClass="com.mmxlabs.models.lng.cargo.CargoGroup"
          targetEClass="com.mmxlabs.models.lng.cargo.Cargo">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.cargo.editor.loadSlotFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Load Slot"
          targetEClass="com.mmxlabs.models.lng.cargo.LoadSlot">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.cargo.editor.dischargeSlotFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Discharge Slot"
          targetEClass="com.mmxlabs.models.lng.cargo.DischargeSlot">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.cargo.editor.desPurchaseSlotFactory"
          factory="com.mmxlabs.models.lng.cargo.ui.modelfactories.DESPurchaseFactory"
          label="DES Purchase Slot"
          targetEClass="com.mmxlabs.models.lng.cargo.LoadSlot">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.cargo.editor.fobSaleSlotFactory"
          factory="com.mmxlabs.models.lng.cargo.ui.modelfactories.FOBSaleFactory"
          label="FOB Sale Slot"
          targetEClass="com.mmxlabs.models.lng.cargo.DischargeSlot">
    </factory>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.editorfactories">
    <editorFactory
          factoryClass="com.mmxlabs.models.ui.dates.TimeInlineEditorFactory"
          featureNameEquals="windowStartTime"
          id="com.mmxlabs.models.lng.cargo.editor.slotTimeEditor">
       <OwnerAnyOf>
          <EClassExtends
                EClass="com.mmxlabs.models.lng.cargo.Slot">
          </EClassExtends>
       </OwnerAnyOf>
    </editorFactory>
 </extension>
 <extension
       point="org.eclipse.ui.views">
    <view
          category="com.mmxlabs.models.lng.views.logical"
          class="com.mmxlabs.models.lng.cargo.editor.views.CargoView"
          id="com.mmxlabs.models.lng.cargo.editor.CargoView"
          name="Cargoes"
          restorable="true">
    </view>
 </extension>
</plugin>
