    <ModelCommandProvider
          commandProvider="com.mmxlabs.models.lng.cargo.ui.commands.DateUpdatingCommandProvider"
          id="com.mmxlabs.models.lng.cargo.ui.commands.DateUpdatingCommandProvider">
    </ModelCommandProvider>
    <ModelCommandProvider
          commandProvider="com.mmxlabs.models.lng.cargo.ui.commands.PortUpdatingCommandProvider"
          id="com.mmxlabs.models.lng.cargo.ui.commands.PortUpdatingCommandProvider">
    </ModelCommandProvider>
    <ModelCommandProvider
          commandProvider="com.mmxlabs.models.lng.cargo.ui.commands.SlotNameUpdatingCommandProvider"
          id="com.mmxlabs.models.lng.cargo.ui.commands.SlotNameUpdatingCommandProvider">
    </ModelCommandProvider>
    <ModelCommandProvider
