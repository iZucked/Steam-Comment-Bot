<extension
      point="org.eclipse.ui.commands">
   <command
         defaultHandler="com.mmxlabs.models.lng.adp.actions.CreateADPCommandHandler"
         id="com.mmxlabs.models.lng.adp.create"
         name="Convert to ADP scenario">
   </command>
</extension>
<extension
      point="org.eclipse.ui.menus">
   <menuContribution
         allPopups="false"
         locationURI="popup:com.mmxlabs.scenario.service.ui.navigator?before=optimisation">
      <command
            commandId="com.mmxlabs.models.lng.adp.create"
            style="push"
            tooltip="Create an ADP model if none exists">
            <visibleWhen
                        checkEnabled="false">
                     <and>
                        <with
                              variable="selection">
                           <iterate
                                 ifEmpty="false">
                              <instanceof
                                    value="com.mmxlabs.scenario.service.model.ScenarioInstance">
                              </instanceof>
                                                   <not><test
                          property="com.mmxlabs.scenario.service.ui.flags.readonly"
                          forcePluginActivation="true">
                     </test></not>

                           </iterate>
                        </with>
                        <count
                              value="1">
                        </count>
						<test
	                        args="features:adp"
	                        forcePluginActivation="true"
                        	property="com.mmxlabs.license.features.enablement">
						</test>
                     </and>
                  </visibleWhen>
      </command>
   </menuContribution>
</extension>
<extension
