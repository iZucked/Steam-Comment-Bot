<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.mmxlabs.com/models/lng/demo-manifest/1/"
            class="com.mmxlabs.models.lng.demo.manifest.ManifestPackage"
            genModel="model/manifest.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="com.mmxlabs.models.lng.demo"
            name="LNG Wizards">
      </category>
      <wizard
            category="com.mmxlabs.models.lng.demo"
            class="com.mmxlabs.models.lng.demo.wizards.EmptyScenarioWizard"
            icon="icons/sample.gif"
            id="com.mmxlabs.models.lng.demo.wizards.EmptyScenarioWizard"
            name="New Empty Scenario">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            file-extensions="scn"
            id="com.mmxlabs.models.lng.demo.scnfile"
            name="SCN File"
            priority="normal">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.mmxlabs.models.ui.editorpart.JointModelEditorPart"
            default="true"
            extensions="scn"
            id="com.mmxlabs.models.lng.demo.scneditor"
            name="SCN Editor">
         <contentTypeBinding
               contentTypeId="com.mmxlabs.models.lng.demo.scnfile">
         </contentTypeBinding>
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.ui.IEditorInput"
            class="com.mmxlabs.models.lng.demo.adapters.JointModelAdapterFactory">
         <adapter
               type="com.mmxlabs.models.mmxcore.jointmodel.JointModel">
         </adapter>
      </factory>
   </extension>

</plugin>
