<plugin>



	
	<extension
		point="com.mmxlabs.models.ui.componenthelpers">
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.PricingModel"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.PricingModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.PricingModel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.DataIndex"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.DataIndexComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.DataIndex" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.DerivedIndex"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.DerivedIndexComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.DerivedIndex" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.IndexPoint"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.IndexPointComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.IndexPoint" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.Index"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.IndexComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.Index" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.FleetCostModel"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.FleetCostModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.FleetCostModel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.RouteCost"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.RouteCostComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.RouteCost" />
		
		
		<componentHelper
			id="com.mmxlabs.models.lng.pricing.helpers.BaseFuelCost"
			helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.BaseFuelCostComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.pricing.BaseFuelCost" />
		
		

  
  <componentHelper
        helperClass="com.mmxlabs.models.lng.pricing.presentation.composites.CooldownPriceComponentHelper"
        id="com.mmxlabs.models.lng.pricing.helpers.CooldownPrice"
        inheritable="false"
        modelClass="com.mmxlabs.models.lng.pricing.CooldownPrice">
  </componentHelper>
		
	</extension>
 <extension
       point="com.mmxlabs.models.ui.valueproviders">
    <valueProvider
          factory="com.mmxlabs.models.lng.pricing.ui.valueproviders.IndexReferenceValueProviderFactory"
          id="com.mmxlabs.models.lng.pricing.editor.indexProvider"
          referenceEClass="com.mmxlabs.models.lng.pricing.Index">
    </valueProvider>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.submodels">
    <submodelImporter
          ID="com.mmxlabs.models.lng.pricing.editor.pricingModelImporter"
          subModelClassName="com.mmxlabs.models.lng.pricing.PricingModel"
          subModelImporterClass="com.mmxlabs.models.lng.pricing.importers.PricingModelImporter">
    </submodelImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.jointmodeleditors">
    <jointModelEditor
          id="com.mmxlabs.models.lng.pricing.editor.pricingModelEditor"
          implementation="com.mmxlabs.models.lng.pricing.ui.editorpart.PricingModelEditorContribution"
          priority="60"
          subModelClass="com.mmxlabs.models.lng.pricing.PricingModel">
    </jointModelEditor>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.classes">
    <classImporter
          ID="com.mmxlabs.models.lng.pricing.editor.dataIndexImporter"
          classImporterClass="com.mmxlabs.models.lng.pricing.importers.DataIndexImporter"
          eClassName="com.mmxlabs.models.lng.pricing.DataIndex"
          inheritable="false">
    </classImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.modelfactories">
    <factory
          ID="com.mmxlabs.models.lng.pricing.editor.dataIndexFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Data Index"
          outputEClass="com.mmxlabs.models.lng.pricing.DataIndex"
          targetEClass="com.mmxlabs.models.lng.pricing.Index">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.pricing.editor.expressionFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Expression"
          outputEClass="com.mmxlabs.models.lng.pricing.DerivedIndex"
          targetEClass="com.mmxlabs.models.lng.pricing.Index">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.pricing.editor.cooldownPricing"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Cooldown Price"
          targetEClass="com.mmxlabs.models.lng.pricing.CooldownPrice">
    </factory>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.componenthelpers">
    <componentHelper
          helperClass="com.mmxlabs.models.lng.pricing.composite_inject.BaseFuelPriceInjector"
          id="com.mmxlabs.models.lng.pricing.editor.basePriceInjector"
          inheritable="false"
          modelClass="com.mmxlabs.models.lng.fleet.BaseFuel">
    </componentHelper>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.classes">
    <classImporter
          ID="com.mmxlabs.models.lng.pricing.editor.portCostImporter"
          classImporterClass="com.mmxlabs.models.lng.pricing.importers.PortCostImporter"
          eClassName="com.mmxlabs.models.lng.pricing.PortCost"
          inheritable="true">
    </classImporter>
    <classImporter
          ID="com.mmxlabs.models.lng.pricing.editor.indexImporter"
          classImporterClass="com.mmxlabs.models.lng.pricing.importers.DataIndexImporter"
          eClassName="com.mmxlabs.models.lng.pricing.Index"
          inheritable="true">
    </classImporter>
 </extension>
 <extension
       point="org.eclipse.ui.views">
    <view
          category="com.mmxlabs.models.lng.views.physical"
          class="com.mmxlabs.models.lng.pricing.ui.views.CooldownCostsView"
          id="com.mmxlabs.models.lng.pricing.editor.CooldownCostsView"
          name="Cooldown Costs"
          restorable="true">
    </view>
    <view
          category="com.mmxlabs.models.lng.views.physical"
          class="com.mmxlabs.models.lng.pricing.ui.views.PortCostsView"
          id="com.mmxlabs.models.lng.pricing.editor.PortCostsView"
          name="Port Costs"
          restorable="true">
    </view>
    <view
          category="com.mmxlabs.models.lng.views.physical"
          class="com.mmxlabs.models.lng.pricing.ui.views.CanalCostsView"
          id="com.mmxlabs.models.lng.pricing.editor.CanalCostsView"
          name="Canal Costs"
          restorable="true">
    </view>
 </extension>
</plugin>
