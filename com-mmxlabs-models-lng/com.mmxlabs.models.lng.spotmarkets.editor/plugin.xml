<plugin>


	
	<extension
		point="com.mmxlabs.models.ui.componenthelpers">
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.SpotMarketsModel"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.SpotMarketsModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.SpotMarketsModel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.CharterCostModel"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.CharterCostModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.CharterCostModel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.SpotMarketGroup"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.SpotMarketGroupComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.SpotMarketGroup" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.SpotMarket"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.SpotMarketComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.SpotMarket" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.DESPurchaseMarket"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.DESPurchaseMarketComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.DESPurchaseMarket" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.DESSalesMarket"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.DESSalesMarketComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.DESSalesMarket" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.FOBPurchasesMarket"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.FOBPurchasesMarketComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.FOBPurchasesMarket" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.FOBSalesMarket"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.FOBSalesMarketComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.FOBSalesMarket" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.spotmarkets.helpers.SpotAvailability"
			helperClass="com.mmxlabs.models.lng.spotmarkets.presentation.composites.SpotAvailabilityComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.spotmarkets.SpotAvailability" />
		
	</extension>
	<extension
       point="org.eclipse.ui.views">
    <view
          category="com.mmxlabs.models.lng.views.physical"
          class="com.mmxlabs.models.lng.spotmarkets.editor.views.FOBSalesSpotMarketView"
          id="com.mmxlabs.models.lng.spotmarkets.editor.views.FOBSalesSpotMarketView"
          name="FOB Sales Spot Markets"
          restorable="true">
    </view>
    <view
          category="com.mmxlabs.models.lng.views.physical"
          class="com.mmxlabs.models.lng.spotmarkets.editor.views.DESPurchaseSpotMarketView"
          id="com.mmxlabs.models.lng.spotmarkets.editor.views.DESPurchaseSpotMarketView"
          name="DES Purchase Spot Markets"
          restorable="true">
    </view>
 </extension>
  <extension
       point="com.mmxlabs.models.util.import.attributes">
    <attributeImporter
          ID="com.mmxlabs.models.lng.spotmarkets.editor.SpotTypeAttributeImporter"
          attributeImporterClass="com.mmxlabs.models.util.importer.impl.DefaultAttributeImporter">
       <dataType
             attributeTypeName="SpotType"
             attributeTypeURI="http://www.mmxlabs.com/models/lng/spotmarkets/1/">
       </dataType>
    </attributeImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.classes">
    <classImporter
          ID="com.mmxlabs.models.lng.spotmarkets.editor.importers.SpotMarketImporter"
          classImporterClass="com.mmxlabs.models.lng.spotmarkets.editor.importers.SpotMarketImporter"
          eClassName="com.mmxlabs.models.lng.spotmarkets.SpotMarket"
          inheritable="true">
    </classImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.modelfactories">
    <factory
          ID="com.mmxlabs.models.lng.spotmarkets.editor.modelfactories.SpotMarketFactory"
          factory="com.mmxlabs.models.lng.spotmarkets.editor.modelfactories.SpotMarketFactory"
          label="Price Expression Spot Market"
          outputEClass="com.mmxlabs.models.lng.commercial.ExpressionPriceParameters"          
          targetEClass="com.mmxlabs.models.lng.spotmarkets.SpotMarket">
    </factory>
        <factory
          ID="com.mmxlabs.models.lng.spotmarkets.editor.CharterCostModelFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Charter Price"
          targetEClass="com.mmxlabs.models.lng.spotmarkets.CharterCostModel">
    </factory>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.jointmodeleditors">
    <jointModelEditor
          id="com.mmxlabs.models.lng.spotmarkets.editor.spotmarketseditor"
          implementation="com.mmxlabs.models.lng.spotmarkets.editor.editorpart.SpotMarketsModelEditorContribution"
          priority="70"
          subModelClass="com.mmxlabs.models.lng.spotmarkets.SpotMarketsModel">
    </jointModelEditor>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.valueproviders">
    <valueProvider
          factory="com.mmxlabs.models.lng.spotmarkets.editor.valueproviders.SpotMarketPortValueProviderFactory"
          id="com.mmxlabs.models.lng..editorspotmarkets.valueproviders.SpotMarketPortValueProviderFactory"
          ownerEClass="com.mmxlabs.models.lng.spotmarkets.SpotMarket"
          referenceEClass="com.mmxlabs.models.lng.port.Port">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.spotmarkets.editor.valueproviders.CharterCostModelIndexProviderFactory"
          id="com.mmxlabs.models.lng.spotmarkets.editor.valueproviders.CharterCostModelIndexProviderFactory"
          ownerEClass="com.mmxlabs.models.lng.spotmarkets.CharterCostModel"
          referenceEClass="com.mmxlabs.models.lng.pricing.Index">
    </valueProvider>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.submodels">
    <submodelImporter
          ID="com.mmxlabs.models.lng.spotmarkets.editor.importers.SpotMartketsModelImporter"
          subModelClassName="com.mmxlabs.models.lng.spotmarkets.SpotMarketsModel"
          subModelImporterClass="com.mmxlabs.models.lng.spotmarkets.editor.importers.SpotMarketsModelImporter">
    </submodelImporter>
 </extension>
</plugin>
