<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="spotmarkets" nsURI="http://www.mmxlabs.com/models/lng/spotmarkets/1/"
    nsPrefix="lng.spotmarkets">
  <eClassifiers xsi:type="ecore:EClass" name="SpotMarketsModel" eSuperTypes="platform:/plugin/com.mmxlabs.models.mmxcore/model/mmxcore.ecore#//UUIDObject">
    <eStructuralFeatures xsi:type="ecore:EReference" name="desPurchaseSpotMarket"
        lowerBound="1" eType="#//SpotMarketGroup" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="desSalesSpotMarket" lowerBound="1"
        eType="#//SpotMarketGroup" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fobPurchasesSpotMarket"
        lowerBound="1" eType="#//SpotMarketGroup" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fobSalesSpotMarket" lowerBound="1"
        eType="#//SpotMarketGroup" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="charteringSpotMarkets"
        upperBound="-1" eType="#//CharterCostModel" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CharterCostModel" eSuperTypes="platform:/plugin/com.mmxlabs.models.mmxcore/model/mmxcore.ecore#//MMXObject">
    <eStructuralFeatures xsi:type="ecore:EReference" name="vesselClasses" upperBound="-1"
        eType="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.fleet/model/fleet.ecore#//VesselClass"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="charterInPrice" lowerBound="1"
        eType="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.pricing/model/pricing.ecore#//CharterIndex"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="charterOutPrice" lowerBound="1"
        eType="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.pricing/model/pricing.ecore#//CharterIndex"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="spotCharterCount" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="minCharterOutDuration"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SpotType">
    <eLiterals name="FOBSale" literal="FOB Sale"/>
    <eLiterals name="DESPurchase" value="1" literal="DES Purchase"/>
    <eLiterals name="DESSale" value="2" literal="DES Sale"/>
    <eLiterals name="FOBPurchase" value="3" literal="FOB Purchase"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SpotMarketGroup" eSuperTypes="platform:/plugin/com.mmxlabs.models.mmxcore/model/mmxcore.ecore#//MMXObject">
    <eStructuralFeatures xsi:type="ecore:EReference" name="availability" lowerBound="1"
        eType="#//SpotAvailability" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1" eType="#//SpotType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="markets" upperBound="-1"
        eType="#//SpotMarket" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SpotMarket" abstract="true" eSuperTypes="platform:/plugin/com.mmxlabs.models.mmxcore/model/mmxcore.ecore#//UUIDObject platform:/plugin/com.mmxlabs.models.mmxcore/model/mmxcore.ecore#//NamedObject">
    <eStructuralFeatures xsi:type="ecore:EReference" name="availability" lowerBound="1"
        eType="#//SpotAvailability" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="minQuantity" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxQuantity" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="priceInfo" eType="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.commercial/model/commercial.ecore#//LNGPriceCalculatorParameters"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DESPurchaseMarket" eSuperTypes="#//SpotMarket">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="cv" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destinationPorts" upperBound="-1">
      <eGenericType eClassifier="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.types/model/lngtypes.ecore#//APortSet">
        <eTypeArguments eClassifier="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.port/model/port.ecore#//Port"/>
      </eGenericType>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DESSalesMarket" eSuperTypes="#//SpotMarket">
    <eStructuralFeatures xsi:type="ecore:EReference" name="notionalPort" lowerBound="1"
        eType="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.port/model/port.ecore#//Port"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FOBPurchasesMarket" eSuperTypes="#//SpotMarket">
    <eStructuralFeatures xsi:type="ecore:EReference" name="notionalPort" lowerBound="1"
        eType="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.port/model/port.ecore#//Port"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="cv" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FOBSalesMarket" eSuperTypes="#//SpotMarket">
    <eStructuralFeatures xsi:type="ecore:EReference" name="loadPort" lowerBound="1"
        eType="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.port/model/port.ecore#//Port"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SpotAvailability">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="constant" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        unsettable="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="curve" lowerBound="1" unsettable="true"
        containment="true">
      <eGenericType eClassifier="ecore:EClass platform:/plugin/com.mmxlabs.models.lng.pricing/model/pricing.ecore#//DataIndex">
        <eTypeArguments eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EIntegerObject"/>
      </eGenericType>
    </eStructuralFeatures>
  </eClassifiers>
</ecore:EPackage>
