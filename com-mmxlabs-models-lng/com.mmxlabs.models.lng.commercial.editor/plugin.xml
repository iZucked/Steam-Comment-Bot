    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.RedirectionPurchaseContractFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Redirection Purchase Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.RedirectionPurchaseContract"
          targetEClass="com.mmxlabs.models.lng.commercial.PurchaseContract">
    </factory>
    -->
    <!-- Sales Contract Factories -->
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.fixedSalesFactory"
          factory="com.mmxlabs.models.lng.commercial.ui.modelfactories.SalesContractModelFactory"
          label="Fixed Sales Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.FixedPriceParameters"
          targetEClass="com.mmxlabs.models.lng.commercial.SalesContract">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.expressionSalesFactory"
          factory="com.mmxlabs.models.lng.commercial.ui.modelfactories.SalesContractModelFactory"
          label="Price Expression Sales Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.ExpressionPriceParameters"
          targetEClass="com.mmxlabs.models.lng.commercial.SalesContract">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.indexSalesFactory"
          factory="com.mmxlabs.models.lng.commercial.ui.modelfactories.SalesContractModelFactory"
          label="Indexed Sales Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.IndexPriceParameters"
          targetEClass="com.mmxlabs.models.lng.commercial.SalesContract">
    </factory>
	<!-- Other Factories -->
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.entityFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Entity"
          targetEClass="com.mmxlabs.models.lng.commercial.LegalEntity">
    </factory>

 </extension>
