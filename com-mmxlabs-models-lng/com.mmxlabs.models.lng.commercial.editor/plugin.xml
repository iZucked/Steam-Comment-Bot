<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.mmxlabs.models.lng.commercial.presentation.CommercialModelWizardID"
            name="%_UI_CommercialModelWizard_label"
            class="com.mmxlabs.models.lng.commercial.presentation.CommercialModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CommercialModelFile.gif">
         <description>%_UI_CommercialModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="com.mmxlabs.models.lng.commercial.presentation.CommercialEditorID"
            name="%_UI_CommercialEditor_label"
            icon="icons/full/obj16/CommercialModelFile.gif"
            extensions="commercial"
            class="com.mmxlabs.models.lng.commercial.presentation.CommercialEditor"
            contributorClass="com.mmxlabs.models.lng.commercial.presentation.CommercialActionBarContributor">
      </editor>
   </extension>

	
	<extension
		point="com.mmxlabs.models.ui.componenthelpers">
		
		<componentHelper
			id="com.mmxlabs.models.lng.commercial.helpers.CommercialModel"
			helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.CommercialModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.commercial.CommercialModel" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.commercial.helpers.LegalEntity"
			helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.LegalEntityComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.commercial.LegalEntity" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.commercial.helpers.Contract"
			helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.ContractComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.commercial.Contract" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.commercial.helpers.SalesContract"
			helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.SalesContractComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.commercial.SalesContract" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.commercial.helpers.PurchaseContract"
			helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.PurchaseContractComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.commercial.PurchaseContract" />
  <componentHelper
        helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.IndexPriceContractComponentHelper"
        id="com.mmxlabs.models.lng.commercial.editor.indexPriceHelper"
        inheritable="false"
        modelClass="com.mmxlabs.models.lng.commercial.IndexPriceContract">
  </componentHelper>
  <componentHelper
        helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.FixedPriceContractComponentHelper"
        id="com.mmxlabs.models.lng.commercial.editor.fixedPriceHelper"
        inheritable="false"
        modelClass="com.mmxlabs.models.lng.commercial.FixedPriceContract">
  </componentHelper>
  <componentHelper
        helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.NetbackPurchaseContractComponentHelper"
        id="com.mmxlabs.models.lng.commercial.editor.netbackPurchaseContractHelper"
        inheritable="false"
        modelClass="com.mmxlabs.models.lng.commercial.NetbackPurchaseContract">
  </componentHelper>
  <componentHelper
        helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.ProfitSharePurchaseContractComponentHelper"
        id="com.mmxlabs.models.lng.commercial.editor.profitSharePurchaseContractHelper"
        inheritable="false"
        modelClass="com.mmxlabs.models.lng.commercial.ProfitSharePurchaseContract">
  </componentHelper>
  <componentHelper
        helperClass="com.mmxlabs.models.lng.commercial.presentation.composites.NotionalBallastParametersComponentHelper"
        id="com.mmxlabs.models.lng.commercial.editor.notionalBallastparametersHelper"
        inheritable="false"
        modelClass="com.mmxlabs.models.lng.commercial.NotionalBallastParameters">
  </componentHelper>
		
	</extension>
 <extension
       point="com.mmxlabs.models.ui.valueproviders">
    <valueProvider
          factory="com.mmxlabs.models.lng.commercial.ui.valueproviders.CommercialProviderFactory"
          id="com.mmxlabs.models.lng.commercial.editor.contractProvider"
          referenceEClass="com.mmxlabs.models.lng.types.AContract">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.commercial.ui.valueproviders.CommercialProviderFactory"
          id="com.mmxlabs.models.lng.commercial.editor.entityProvider"
          referenceEClass="com.mmxlabs.models.lng.types.ALegalEntity">
    </valueProvider>
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.submodels">
    <submodelImporter
          ID="com.mmxlabs.models.lng.commercial.editor.commercialModelImporter"
          subModelClassName="com.mmxlabs.models.lng.commercial.CommercialModel"
          subModelImporterClass="com.mmxlabs.models.lng.commercial.importer.CommercialModelImporter">
    </submodelImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.jointmodeleditors">
    <jointModelEditor
          id="com.mmxlabs.models.lng.commercial.editor.jointModelEditorContribution"
          implementation="com.mmxlabs.models.lng.commercial.ui.editorpart.CommercialModelEditorContribution"
          priority="5"
          subModelClass="com.mmxlabs.models.lng.commercial.CommercialModel">
    </jointModelEditor>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.modelfactories">
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.fixedPurchaseFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Fixed Purchase Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.FixedPriceContract"
          targetEClass="com.mmxlabs.models.lng.commercial.PurchaseContract">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.fixedSalesFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Fixed Sales Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.FixedPriceContract"
          targetEClass="com.mmxlabs.models.lng.commercial.SalesContract">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.indexPurchaseFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Indexed Purchase Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.IndexPriceContract"
          targetEClass="com.mmxlabs.models.lng.commercial.PurchaseContract">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.indexSalesFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Indexed Sales Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.IndexPriceContract"
          targetEClass="com.mmxlabs.models.lng.commercial.SalesContract">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.entityFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Entity"
          targetEClass="com.mmxlabs.models.lng.commercial.LegalEntity">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.NetbackPurchaseContractFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Netback Purchase Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.NetbackPurchaseContract"
          targetEClass="com.mmxlabs.models.lng.commercial.PurchaseContract">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.commercial.editor.ProfitSharePurchaseContractFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Profit Share Purchase Contract"
          outputEClass="com.mmxlabs.models.lng.commercial.ProfitSharePurchaseContract"
          targetEClass="com.mmxlabs.models.lng.commercial.PurchaseContract">
    </factory>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.modelfactories">
    <factory
          ID="com.mmxlabs.models.lng.commercial.NotionalBallastParametersFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Notional Ballast Parameters"
          outputEClass="com.mmxlabs.models.lng.commercial.NotionalBallastParameters"
          targetEClass="com.mmxlabs.models.lng.commercial.NotionalBallastParameters">
    </factory>
 </extension>
</plugin>
