<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.mmxlabs.models.lng.port.presentation.PortModelWizardID"
            name="%_UI_PortModelWizard_label"
            class="com.mmxlabs.models.lng.port.presentation.PortModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PortModelFile.gif">
         <description>%_UI_PortModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="com.mmxlabs.models.lng.port.presentation.PortEditorID"
            name="%_UI_PortEditor_label"
            icon="icons/full/obj16/PortModelFile.gif"
            extensions="port"
            class="com.mmxlabs.models.lng.port.presentation.PortEditor"
            contributorClass="com.mmxlabs.models.lng.port.presentation.PortActionBarContributor">
      </editor>
   </extension>

	
	<extension
		point="com.mmxlabs.models.ui.componenthelpers">
		
		<componentHelper
			id="com.mmxlabs.models.lng.port.helpers.Port"
			helperClass="com.mmxlabs.models.lng.port.presentation.composites.PortComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.port.Port" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.port.helpers.Route"
			helperClass="com.mmxlabs.models.lng.port.presentation.composites.RouteComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.port.Route" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.port.helpers.PortGroup"
			helperClass="com.mmxlabs.models.lng.port.presentation.composites.PortGroupComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.port.PortGroup" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.port.helpers.RouteLine"
			helperClass="com.mmxlabs.models.lng.port.presentation.composites.RouteLineComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.port.RouteLine" />
		
		<componentHelper
			id="com.mmxlabs.models.lng.port.helpers.PortModel"
			helperClass="com.mmxlabs.models.lng.port.presentation.composites.PortModelComponentHelper"
			inheritable="false"
			modelClass="com.mmxlabs.models.lng.port.PortModel" />
		
	</extension>
 <extension
       point="com.mmxlabs.models.ui.valueproviders">
    <valueProvider
          factory="com.mmxlabs.models.lng.port.ui.valueproviders.PortValueProviderFactory"
          id="com.mmxlabs.models.lng.port.editor.portValueProvider"
          referenceEClass="com.mmxlabs.models.lng.types.APortSet">
    </valueProvider>
    <valueProvider
          factory="com.mmxlabs.models.lng.port.ui.valueproviders.RouteValueProviderFactory"
          id="com.mmxlabs.models.lng.port.editor.routeValueProvider"
          referenceEClass="com.mmxlabs.models.lng.types.ARoute">
    </valueProvider>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.jointmodeleditors">
    <jointModelEditor
          id="com.mmxlabs.models.lng.port.portModelEditorContribution"
          implementation="com.mmxlabs.models.lng.port.ui.editorpart.PortModelEditorContribution"
          priority="2"
          subModelClass="com.mmxlabs.models.lng.port.PortModel">
    </jointModelEditor>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.jointmodeleditors">
 </extension>
 <extension
       point="com.mmxlabs.models.util.import.submodels">
    <submodelImporter
          ID="com.mmxlabs.models.lng.port.editor.portModelImporter"
          subModelClassName="com.mmxlabs.models.lng.port.PortModel"
          subModelImporterClass="com.mmxlabs.models.lng.port.importer.PortModelImporter">
    </submodelImporter>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.modelfactories">
    <factory
          ID="com.mmxlabs.models.lng.port.editor.portFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Port"
          targetEClass="com.mmxlabs.models.lng.port.Port">
    </factory>
    <factory
          ID="com.mmxlabs.models.lng.port.editor.portGroupFactory"
          factory="com.mmxlabs.models.ui.modelfactories.DefaultModelFactory"
          label="Port Group"
          targetEClass="com.mmxlabs.models.lng.port.PortGroup">
    </factory>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.editorfactories">
    <editorFactory
          factoryClass="com.mmxlabs.models.ui.dates.TimeInlineEditorFactory"
          featureNameEquals="defaultStartTime"
          id="com.mmxlabs.models.lng.port.editor.portTimeEditor">
       <OwnerAnyOf>
          <EClassExtends
                EClass="com.mmxlabs.models.lng.port.Port">
          </EClassExtends>
       </OwnerAnyOf>
    </editorFactory>
 </extension>
 <extension
       point="org.eclipse.ui.views">
    <view
          category="com.mmxlabs.models.lng.views.physical"
          class="com.mmxlabs.models.lng.port.editor.views.PortView"
          icon="icons/sample.gif"
          id="com.mmxlabs.models.lng.port.editor.views.PortView"
          name="Ports">
    </view>
    <view
          category="com.mmxlabs.models.lng.views.physical"
          class="com.mmxlabs.models.lng.port.editor.views.PortGroupView"
          icon="icons/sample.gif"
          id="com.mmxlabs.models.lng.port.editor.views.PortGroupView"
          name="Port Groups">
    </view>
 </extension>
 <extension
       point="org.eclipse.help.contexts">
    <contexts
          file="contexts.xml">
    </contexts>
 </extension>
 <extension
       point="org.eclipse.ui.menus">
    <menuContribution
          allPopups="false"
          locationURI="toolbar:com.mmxlabs.models.lng.port.ui.editorpart.PortEditorPane">
       <command
             commandId="org.eclipse.ui.views.showView"
             icon="icons/group.gif"
             label="Show Port Groups"
             style="push"
             tooltip="Show Port Group Editor">
          <parameter
                name="org.eclipse.ui.views.showView.makeFast"
                value="true">
          </parameter>
          <parameter
                name="org.eclipse.ui.views.showView.viewId"
                value="com.mmxlabs.models.lng.port.editor.views.PortGroupView">
          </parameter>
       </command>
    </menuContribution>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.editorfactories">
    <editorFactory
          factoryClass="com.mmxlabs.models.lng.port.ui.editors.PortGroupContentsEditorFactory"
          featureNameEquals="contents"
          id="com.mmxlabs.models.lng.port.editor.portGroupEditorFactory">
       <OwnerAnyOf>
          <EClassExtends
                EClass="com.mmxlabs.models.lng.port.PortGroup">
          </EClassExtends>
       </OwnerAnyOf>
    </editorFactory>
 </extension>
 <extension
       point="com.mmxlabs.models.ui.displaycompositefactories">
    <displayCompositeFactory
          eClass="com.mmxlabs.models.lng.port.PortGroup"
          factory="com.mmxlabs.models.lng.port.ui.editors.PortGroupCompositeFactory"
          id="com.mmxlabs.models.lng.port.editor.portGroupCompositeFactory"
          inheritable="true">
    </displayCompositeFactory>
 </extension>
</plugin>
